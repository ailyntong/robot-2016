CheckArmCalibration
SetWedge: false
Shift: false

//Saves time by going to position while driving
COMPLETE ALL:
  SetArmPosition: INTAKE: 2
  DriveStraight: 12.0, false

//Inital turn for shot
SetArmPosition: AUTO: 3
PointTurn: angle: 25.0

//Raise wedge up
SetWedge: true
Wait: seconds: 0.4
Align
Wait: 0.3
Shoot

//Go to the side
AbsolutePointTurn: -90.0
DriveStraight: 5.5, false
PointTurn: angle: 80.0

//Drive over defense on left
Shift: false
DriveStraight: -11.0, false
